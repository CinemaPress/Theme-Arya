<!DOCTYPE html>
<html id="html" lang="<%- page.language %>" class="no-mobile js flexbox flexboxlegacy touch cssanimations csstransforms csstransforms3d csstransitions svg">
<head>

    <% include includes/head.ejs %>

</head>
<body class="list-page backdropped">

<% include includes/header.ejs %>

<div id="content" class="has-backdrop">
    <% var bg = ''; if (typeof movies === 'object' && movies.length) {
        movies.slice(0,1).forEach(function (movie) { bg = movie.picture; }); } %>
    <div id="backdrop" style="background-position: center -90px; background-image: url(<%- bg %>);">
        <div class="backdrop-fade backdrop-fade-image">
            <div class="content-wrap">
                <div class="cols-2">
                    <section class="section col-17 col-main overflow clearfix">

                        <% if (typeof page.adv === 'object' && page.adv.top) { %>
                        <div style="text-align: center; margin: 10px auto">
                            <%- page.adv.top %>
                        </div>
                        <% } %>

                        <div class="list-title-intro">
                            <h1 class="title-1 prettify"><%- page.h1 %></h1>

                            <div class="body-text -prose -hero clear collapsible-text">
                                <div class="collapsed-text">
                                <% if (typeof content === 'object') { %>
                                    <% if (content.image) { %>
                                    <img src="<%- content.image %>" style="height: 150px;border-radius: 5px;float: left;margin-right: 10px;margin-bottom: 10px; margin-top: 8px;">
                                    <% } %>
                                    <% if (content.description) { %>
                                    <div>
                                        <%- content.description %>
                                    </div>
                                    <% } %>
                                <% } else if (page.seo) { %>
                                    <%- page.seo %>
                                <% } %>
                                </div>
                            </div>

                        </div>

                        <% if (typeof page.sorting === 'object') { %>
                        <div class="clear"></div>
                        <div id="content-nav" class="has-toggle">
                            <div class="sorting-selects has-hide-toggle">
                                <section class="smenu-wrapper">
                                    <div class="smenu">
                                        <% page.sorting.forEach(function (sort) { %>
                                        <% if (sort.active) { %>
                                        <a href="<%- sort.url %>">
                                            <strong><%- sort.name %></strong>
                                        </a>
                                        <% } else { %>
                                        <a href="<%- sort.url %>">
                                            <%- sort.name %>
                                        </a>
                                        <% } }); %>
                                    </div>
                                </section>
                            </div>
                        </div>
                        <div class="clear"></div>
                        <% } %>

                        <% if (typeof movies === 'object' && movies.length) { %>
                        <ul class="poster-list -p70 film-list clear film-details-list">

                            <% movies.forEach(function (movie) { %>
                            <li class="film-detail">
                                <div class="poster film-poster really-lazy-load">
                                    <img src="<%- movie.poster_min %>" class="image" width="70" height="105" alt="<%= movie.title %>"/>
                                    <span class="frame"><span class="frame-title"></span></span>
                                </div>
                                <div class="film-detail-content">
                                    <h2 class="headline-2 prettify">
                                        <a href="<%- movie.url %>"><%= movie.title %></a>
                                        <small class="metadata">
                                            <%- movie.year_url %>
                                        </small>
                                    </h2>
                                    <p class="film-detail-meta">
                                        <%- movie.description_short %>...
                                    </p>
                                </div>
                            </li>
                            <% }); %>

                        </ul>
                        <% } %>

                        <% if (typeof page.pagination === 'object') { %>
                        <div class="pagination">
                            <% if (page.pagination.first) { %>
                            <div class="paginate-nextprev">
                                <a class="previous" href="<%- page.pagination.first.link %>"><%- page.pagination.first.number %></a>
                            </div>
                            <% } %>
                            <% if (page.pagination.last) { %>
                            <div class="paginate-nextprev">
                                <a class="next" href="<%- page.pagination.last.link %>"><%- page.pagination.last.number %></a>
                            </div>
                            <% } %>
                            <div class="paginate-pages">
                                <ul>
                                    <% if (page.pagination.prev.length) { %>
                                    <% page.pagination.prev.forEach(function (prev) { %>
                                    <li class="paginate-page"><a href="<%- prev.link %>"><%- prev.number %></a></li>
                                    <% }); %>
                                    <% } %>
                                    <li class="paginate-page paginate-current"><span><%- page.pagination.current %></span></li>
                                    <% if (page.pagination.next.length) { %>
                                    <% page.pagination.next.forEach(function (next) { %>
                                    <li class="paginate-page"><a href="<%- next.link %>"><%- next.number %></a></li>
                                    <% }); %>
                                    <% } %>
                                </ul>
                            </div>
                        </div>
                        <% } %>

                        <% if (typeof content === 'object' && page.comments) { %>
                        <div id="comments" class="clear has-comment-controls">
                            <h2 class="section-heading">
                                <span class="comment-count"><%- page.l.comments %></span>
                            </h2>
                            <%- page.comments %>
                        </div>
                        <% } %>

                        <div class="clear"></div>
                    </section>
                    <aside class="sidebar">
                        <section id="userpanel" class="actions-panel">
                            <ul class="js-actions-panel">
                                <li class="panel-signin">
                                    <%- page.l.share %>
                                </li>
                                <li class="panel-share">
                                    <a class="resp-sharing-button__link" href="https://facebook.com/sharer/sharer.php?u=http%3A%2F%2Fsharingbuttons.io" target="_blank" aria-label="">
                                        <div class="resp-sharing-button resp-sharing-button--facebook resp-sharing-button--small"><div aria-hidden="true" class="resp-sharing-button__icon resp-sharing-button__icon--solid">
                                                <img src="data:image/svg+xml;utf8;base64," />
                                            </div>
                                        </div>
                                    </a>
                                    <a class="resp-sharing-button__link" href="https://twitter.com/intent/tweet/?text=Super%20fast%20and%20easy%20Social%20Media%20Sharing%20Buttons.%20No%20JavaScript.%20No%20tracking.&amp;url=http%3A%2F%2Fsharingbuttons.io" target="_blank" aria-label="">
                                        <div class="resp-sharing-button resp-sharing-button--twitter resp-sharing-button--small"><div aria-hidden="true" class="resp-sharing-button__icon resp-sharing-button__icon--solid">
                                                <img src="data:image/svg+xml;utf8;base64," />
                                            </div>
                                        </div>
                                    </a>
                                    <a class="resp-sharing-button__link" href="https://plus.google.com/share?url=http%3A%2F%2Fsharingbuttons.io" target="_blank" aria-label="">
                                        <div class="resp-sharing-button resp-sharing-button--google resp-sharing-button--small"><div aria-hidden="true" class="resp-sharing-button__icon resp-sharing-button__icon--solid">
                                                <img src="data:image/svg+xml;utf8;base64," />
                                            </div>
                                        </div>
                                    </a>
                                    <a class="resp-sharing-button__link" href="https://vk.com/share.php?url=http%3A%2F%2Fsharingbuttons.io" target="_blank" aria-label="">
                                        <div class="resp-sharing-button resp-sharing-button--vk resp-sharing-button--small"><div aria-hidden="true" class="resp-sharing-button__icon resp-sharing-button__icon--solid">
                                                <img src="data:image/svg+xml;utf8;base64," />
                                            </div>
                                        </div>
                                    </a>
                                </li>
                            </ul>
                        </section>

                        <% if (typeof content === 'object' && content.tags) { %>
                        <section class="section" style="margin-bottom: 20px">
                            <h3 class="section-heading"><%- page.l.tags %></h3>
                            <ul class="tags">
                                <li>
                                    <%- content.tags %>
                                </li>
                            </ul>
                        </section>
                        <% } %>

                        <% if (typeof news === 'object' && news.length) { %>
                        <section class="section list-stacked-narrow">
                            <h2 class="section-heading"><%- page.l.news %></h2>
                            <ul class="poster-list -p70 featured-list">
                                <% news.forEach(function (content) { %>
                                <li class="list -overlapped -stacked">
                                    <a href="<%- content.url %>" class="list-link">
                                        <div class="list-link-stacked clear">
                                            <ul class="poster-list -overlapped -p70">
                                                <% content.movies.slice(0,5).forEach(function (id) { %>
                                                <li class="react-component film-poster poster">
                                                    <div>
                                                        <img src="https://cdn.cinemapress.org/images/film_iphone/iphone_<%- id %>.jpg?width=60" width="70" height="105" alt="<%= content.title %>" class="image">
                                                    </div>
                                                </li>
                                                <% }); %>
                                            </ul>
                                        </div>
                                        <span class="overlay"></span>
                                    </a>
                                    <h3 class="title-3 prettify">
                                        <a href="<%- content.url %>"><%= content.title %></a>
                                    </h3>
                                </li>
                                <% }); %>
                            </ul>
                        </section>
                        <% } %>

                        <% if (typeof page.adv === 'object' && page.adv.right) { %>
                        <div style="text-align: center; margin: 10px auto">
                            <%- page.adv.right %>
                        </div>
                        <% } %>

                    </aside>
                </div>

                <% if (typeof page.adv === 'object' && page.adv.bottom) { %>
                <div style="text-align: center; margin: 10px auto">
                    <%- page.adv.bottom %>
                </div>
                <% } %>

            </div>
        </div>
    </div>
</div>
<footer class="page-footer js-page-footer">

    <% include includes/footer.ejs %>

</footer>
</body>
</html>