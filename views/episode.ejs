<!DOCTYPE html>
<html id="html" lang="<%- page.language %>" class="no-mobile js flexbox flexboxlegacy touch cssanimations csstransforms csstransforms3d csstransitions svg">
<head>

    <% include includes/head.ejs %>

</head>
<body class="film backdropped logged-out touch backdrop-loaded" data-type="film">

<% include includes/header.ejs %>

<div id="content" class="has-backdrop">
    <div id="backdrop" style="background-image: url(<%- movie.picture_big %>); background-position: 50% -20px;">
        <div class="backdrop-fade backdrop-fade-image">
            <div class="content-wrap">
                <div id="film-page-wrapper" class="cols-3 overflow">
                    <div id="poster-col" class="col-6 gutter-right-1 col-poster-large">
                        <section class="poster-list -p230 no-hover el col">
                            <div class="react-component film-poster poster">
                                <div>
                                    <img src="<%- movie.poster %>" width="230" height="345" alt="<%= movie.title %>" class="image">
                                    <span class="frame">
                                        <span class="frame-title"><%= movie.title %> (<%- movie.year %>)</span>
                                    </span>
                                </div>
                            </div>
                        </section>
                        <section class="watch-panel js-watch-panel">
                            <h3 class="title">Разделы</h3>
                            <p class="js-watch-panel-trailer">
                                <a class="label -text track-event js-trailer-zoom cboxElement" href="<%- movie.url %>">
                                    <span class="icon -play"></span>
                                    <span class="name">Информация</span>
                                </a>
                            </p>
                            <% if (page.urls.movies.online) { %>
                            <p class="js-watch-panel-trailer">
                                <a class="label -text track-event js-trailer-zoom cboxElement" href="<%- movie.url %>/<%- page.urls.movies.online %>">
                                    <span class="icon -play"></span>
                                    <span class="name">Онлайн</span>
                                </a>
                            </p>
                            <% } %>
                            <% if (page.urls.movies.download) { %>
                            <p class="js-watch-panel-trailer">
                                <a class="label -text track-event js-trailer-zoom cboxElement" href="<%- movie.url %>/<%- page.urls.movies.download %>">
                                    <span class="icon -play"></span>
                                    <span class="name">Скачать</span>
                                </a>
                            </p>
                            <% } %>
                            <% if (page.urls.movies.trailer) { %>
                            <p class="js-watch-panel-trailer">
                                <a class="label -text track-event js-trailer-zoom cboxElement" href="<%- movie.url %>/<%- page.urls.movies.trailer %>">
                                    <span class="icon -play"></span>
                                    <span class="name">Трейлер</span>
                                </a>
                            </p>
                            <% } %>
                            <% if (page.urls.movies.picture) { %>
                            <p class="js-watch-panel-trailer">
                                <a class="label -text track-event js-trailer-zoom cboxElement" href="<%- movie.url %>/<%- page.urls.movies.picture %>">
                                    <span class="icon -play"></span>
                                    <span class="name">Кадры</span>
                                </a>
                            </p>
                            <% } %>
                        </section>

                        <% if (typeof news === 'object' && news.length) { %>
                        <section class="section list-stacked-narrow" style="margin-top: 2.30769231rem; padding: 0">
                            <h2 class="section-heading">Новое</h2>
                            <ul class="poster-list -p70 featured-list">
                                <% news.forEach(function (content) { %>
                                <li class="list -overlapped -stacked">
                                    <a href="<%- content.url %>" class="list-link">
                                        <div class="list-link-stacked clear">
                                            <ul class="poster-list -overlapped -p70">
                                                <% content.movies.slice(0,5).forEach(function (id) { %>
                                                <li class="react-component film-poster poster">
                                                    <div>
                                                        <img src="https://cdn.cinemapress.org/images/film_iphone/iphone_<%- id %>.jpg?width=60" width="70" height="105" alt="<%= content.title %>" class="image">
                                                    </div>
                                                </li>
                                                <% }); %>
                                            </ul>
                                        </div>
                                        <span class="overlay"></span>
                                    </a>
                                    <h3 class="title-3 prettify">
                                        <a href="<%- content.url %>"><%= content.title %></a>
                                    </h3>
                                </li>
                                <% }); %>
                            </ul>
                        </section>
                        <% } %>

                        <% if (typeof page.adv === 'object' && page.adv.left) { %>
                        <div style="text-align: center; margin: 10px auto">
                            <%- page.adv.left %>
                        </div>
                        <% } %>

                    </div>
                    <div class="col-17">

                        <section id="featured-film-header">
                            <h1 class="headline-1 js-widont prettify"><%= movie.title %> серия</h1>
                            <p>
                                <small class="number">
                                    <%- movie.year_url %>
                                </small>
                                <% if (movie.director_url) { %>
                                Режиссер <span class="prettify"><%- movie.director_url %></span>
                                <% } %>
                            </p>
                        </section>

                        <% if (typeof page.adv === 'object' && page.adv.top) { %>
                        <div style="text-align: center; margin-bottom: 20px;">
                            <%- page.adv.top %>
                        </div>
                        <% } %>

                        <section class="section col-10 col-main">
                            <div id="tabbed-content">
                                <div id="tab-details" class="tabbed-content-block column-block">
                                    <% if (movie.countries_url) { %>
                                    <h3><span>Страна</span></h3>
                                    <div class="text-sluglist">
                                        <p>
                                            <%- movie.countries_url %>
                                        </p>
                                    </div>
                                    <% } %>
                                    <% if (movie.genres_url) { %>
                                    <h3><span>Жанры</span></h3>
                                    <div class="text-sluglist">
                                        <p>
                                            <%- movie.genres_url %>
                                        </p>
                                    </div>
                                    <% } %>
                                    <% if (movie.actors_url) { %>
                                    <h3><span>Актеры</span></h3>
                                    <div class="text-sluglist">
                                        <p>
                                            <%- movie.actors_url %>
                                        </p>
                                    </div>
                                    <% } %>
                                </div>
                            </div>
                            <% if (page.seo) { %>
                            <section>
                                <div class="review body-text -prose -hero prettify">
                                    <h3 class="hidden">Описание</h3>
                                    <div class="truncate">
                                        <%- page.seo %>
                                    </div>
                                </div>
                            </section>
                            <% } %>
                            <p class="text-link text-footer">
                                <% if (movie.quality) { %>
                                <%- movie.quality %>
                                <% } %> <% if (movie.translate) { %>
                                <%- movie.translate %>
                                <% } %>
                            </p>
                        </section>
                        <aside class="sidebar">
                            <section id="userpanel" class="actions-panel">
                                <ul class="js-actions-panel">
                                    <li class="panel-signin">
                                        Поделиться
                                    </li>
                                    <li class="panel-share">
                                        <a class="resp-sharing-button__link" href="https://facebook.com/sharer/sharer.php?u=<%- encodeURIComponent(movie.url) %>" target="_blank" aria-label="">
                                          <div class="resp-sharing-button resp-sharing-button--facebook resp-sharing-button--small"><div aria-hidden="true" class="resp-sharing-button__icon resp-sharing-button__icon--solid">
                                            <img src="data:image/svg+xml;utf8;base64," />
                                            </div>
                                          </div>
                                        </a>
                                        <a class="resp-sharing-button__link" href="https://twitter.com/intent/tweet/?text=<%- encodeURIComponent(movie.url) %>&amp;url=<%- encodeURIComponent(movie.url) %>" target="_blank" aria-label="">
                                          <div class="resp-sharing-button resp-sharing-button--twitter resp-sharing-button--small"><div aria-hidden="true" class="resp-sharing-button__icon resp-sharing-button__icon--solid">
                                            <img src="data:image/svg+xml;utf8;base64," />
                                            </div>
                                          </div>
                                        </a>
                                        <a class="resp-sharing-button__link" href="https://plus.google.com/share?url=<%- encodeURIComponent(movie.url) %>" target="_blank" aria-label="">
                                          <div class="resp-sharing-button resp-sharing-button--google resp-sharing-button--small"><div aria-hidden="true" class="resp-sharing-button__icon resp-sharing-button__icon--solid">
                                            <img src="data:image/svg+xml;utf8;base64," />
                                            </div>
                                          </div>
                                        </a>
                                        <a class="resp-sharing-button__link" href="https://vk.com/share.php?url=<%- encodeURIComponent(movie.url) %>" target="_blank" aria-label="">
                                          <div class="resp-sharing-button resp-sharing-button--vk resp-sharing-button--small"><div aria-hidden="true" class="resp-sharing-button__icon resp-sharing-button__icon--solid">
                                            <img src="data:image/svg+xml;utf8;base64," />
                                            </div>
                                          </div>
                                        </a>
                                    </li>
                                </ul>
                            </section>

                            <% if (movie.vote) { %>
                            <section class="section ratings-histogram-chart">
                                <h2 class="section-heading">
                                    <a href="javascript:void(0)" class="tooltip" title="Рейтинг">Рейтинг</a>
                                </h2>
                                <% if (movie.kp_vote) { %>
                                <span class="average-rating" style="position: initial">
                                    <span class="display-rating" style="float: right; color: #FF6600">КиноПоиск <%- movie.kp_rating %>%</span>
                                    <span class="rating-green rating-green-tiny rating-5">
                                        <% for (var i = 1; i <= movie.kp_rating/10; i++) { %><span class="rating" style="width: 9px; height: 9px;">★</span><% } %>
                                    </span>
                                </span>
                                <div style="clear: both"></div>
                                <% } %>
                                <% if (movie.imdb_vote) { %>
                                <span class="average-rating" style="position: initial">
                                    <span class="display-rating" style="float: right; color: #E6BE2E">IMDb <%- movie.imdb_rating %>%</span>
                                    <span class="rating-green rating-green-tiny rating-5">
                                        <% for (var i = 1; i <= movie.imdb_rating/10; i++) { %><span class="rating" style="width: 9px; height: 9px;">★</span><% } %>
                                    </span>
                                </span>
                                <div style="clear: both"></div>
                                <% } %>
                                <span class="average-rating" style="position: initial">
                                    <span class="display-rating" style="float: right; color: #50C375">Общий <%- movie.rating %>%</span>
                                    <span class="rating-green rating-green-tiny rating-5">
                                        <% for (var i = 1; i <= movie.rating/10; i++) { %><span class="rating" style="width: 9px; height: 9px;">★</span><% } %>
                                    </span>
                                </span>
                            </section>
                            <% } %>

                            <% if (typeof soon === 'object' && soon.length) { %>
                            <section class="section clearfix">
                                <h2 class="section-heading heading-no-border">
                                    Скоро премьеры
                                </h2>
                                <div class="poster-list -p70">
                                    <% soon.slice(0,1).forEach(function (movie) { %>
                                    <div class="film-prevnext -next">
                                        <div class="react-component film-poster poster linked-film-poster">
                                            <div>
                                                <img src="<%- movie.poster_min %>" alt="<%= movie.title %>" class="image" height="105" width="70">
                                                <a href="<%- movie.url %>" class="frame">
                                                    <span class="frame-title"><%= movie.title %> (<%- movie.year %>)</span>
                                                    <span class="overlay"></span>
                                                </a>
                                            </div>
                                        </div>
                                    </div>
                                    <% }); %>
                                    <% soon.slice(1,2).forEach(function (movie) { %>
                                    <div class="film-prevnext -prev">
                                        <div class="react-component film-poster poster linked-film-poster">
                                            <div>
                                                <img src="<%- movie.poster_min %>" alt="<%= movie.title %>" class="image" height="105" width="70">
                                                <a href="<%- movie.url %>" class="frame">
                                                    <span class="frame-title"><%= movie.title %> (<%- movie.year %>)</span>
                                                    <span class="overlay"></span>
                                                </a>
                                            </div>
                                        </div>
                                    </div>
                                    <% }); %>
                                </div>
                            </section>
                            <% } %>

                            <% if (typeof page.adv === 'object' && page.adv.right) { %>
                            <div style="text-align: center; margin: 10px auto">
                                <%- page.adv.right %>
                            </div>
                            <% } %>

                            <section class="section friends-watched watchLaterBlock" style="display: none">
                                <h2 class="section-heading">
                                    Досмотреть позже?
                                </h2>
                                <p class="has-icon icon-16 icon-watched small-watched no-avatars">
                                    <span class="icon"></span>
                                    <a href="javascript:void(0)" id="watchLater">Да, досмотреть позже</a>
                                </p>
                            </section>
                        </aside>

                        <section class="film-recent-reviews episodesListBlock" style="display: none; margin: 10px auto;">
                            <h3><%= movie.title %> новые серии</h3>
                            <div id="episodesList" data-id="<%- movie.kp_id %>"></div>
                        </section>

                        <% if (typeof page.adv === 'object' && page.adv.over) { %>
                        <section class="film-recent-reviews" style="position: relative; text-align: center; margin: 10px auto">
                            <%- page.adv.over %>
                        </section>
                        <% } %>

                        <section class="film-recent-reviews" style="position: relative; margin: 10px auto; border-radius: 10px;background: 0 0;overflow: hidden;">
                            <%- page.player %>
                        </section>

                        <% if (typeof page.adv === 'object' && page.adv.under) { %>
                        <section class="film-recent-reviews" style="position: relative; text-align: center; margin: 10px auto">
                            <%- page.adv.under %>
                        </section>
                        <% } %>

                        <% if (page.comments) { %>
                        <section class="film-recent-reviews">
                            <section id="popular-reviews" class="film-reviews section">
                                <ul class="film-popular-review">
                                    <li class="film-detail">
                                        <%- page.comments %>
                                    </li>
                                </ul>
                            </section>
                        </section>
                        <% } %>

                        <% if (typeof movies === 'object') {
                            for (var type in movies) {
                                if (movies.hasOwnProperty(type) && movies[type].length) {
                                    movies[type].forEach(function (category) { %>
                        <section id="related" class="section related-films">
                            <h2 class="section-heading"><%- category.name %></h2>
                            <ul class="poster-list -p110 -horizontal -scaled104">
                                <% if (category.movies.length) {
                                category.movies.slice(0,6).forEach(function (movie) { %>
                                <li class="poster-container">
                                    <div class="poster film-poster really-lazy-load">
                                        <img src="<%- movie.poster %>" class="image" width="110" height="165" alt="<%= movie.title %>">
                                        <a href="<%- movie.url %>" class="frame">
                                            <span class="frame-title"><%= movie.title %> (<%- movie.year %>)</span>
                                            <span class="overlay"></span>
                                        </a>
                                    </div>
                                </li>
                                <% }); } %>
                            </ul>
                        </section>
                        <% }); } } } %>

                        <% if (typeof contents === 'object' && contents.length) { %>
                        <section id="lists" class="list-set section">
                            <h2 class="section-heading">Упоминания</h2>

                            <% contents.forEach(function (content) { %>
                            <section class="list -overlapped -summary ">
                                <a href="<%- content.url %>" class="list-link">
                                    <ul class="poster-list -p70 -overlapped">

                                        <% content.movies.slice(0,5).forEach(function (id) { %>
                                        <li class="poster film-poster really-lazy-load listitem">
                                            <img src="https://cdn.cinemapress.org/images/film_iphone/iphone_<%- id %>.jpg?width=60" class="image" width="70" height="105" alt="<%= content.title %>">
                                            <span class="frame"><span class="frame-title"></span></span>
                                        </li>
                                        <% }); %>

                                    </ul>
                                </a>
                                <span class="overlay"></span>
                                <div class="film-list-summary">
                                    <h2 class="title-2 prettify">
                                        <a href="<%- content.url %>"><%= content.title %></a>
                                    </h2>
                                    <div class="attribution-block">
                                        <div class="body-text -prose"><%- content.description_short %>...</div>
                                    </div>
                                </div>
                            </section>
                            <% }); %>

                        </section>
                        <% } %>

                    </div>

                    <div class="clear"></div>

                    <% if (typeof page.adv === 'object' && page.adv.bottom) { %>
                    <div style="text-align: center; margin: 10px auto">
                        <%- page.adv.bottom %>
                    </div>
                    <% } %>

                    <div class="clear"></div>
                </div>
            </div>
        </div>
    </div>
</div>
<footer class="page-footer js-page-footer">

    <% include includes/footer.ejs %>

</footer>
</body>
</html>